<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:p1="http://www.owl-ontologies.com/assert.owl#"
    xmlns="http://www.owl-ontologies.com/TravelOnt08#"
  xml:base="http://www.owl-ontologies.com/TravelOnt08">
  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Travel agent ontology. Created by Konstantinos Tzonas and Dimitrios Freskas</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >0.8</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="Accommodation">
    <owl:disjointWith>
      <owl:Class rdf:ID="TransportationMeans"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="TravelPath"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:ID="availableFacility"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Facility"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Location"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Service"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A class that describes lodging facilities (e.g., hotels, camping sites etc)</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Activity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Route"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="locatedAt"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#Location"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Facility"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="TravelPackage"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="AccommodationWithSpa">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="SpaFacility"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="#availableFacility"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="#Accommodation"/>
  </owl:Class>
  <owl:Class rdf:ID="Tour">
    <owl:disjointWith>
      <owl:Class rdf:ID="VehicleRental"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Service"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SpaPackage">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasAccommodation"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#AccommodationWithSpa"/>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TravelPackage"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="TrainOnly">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="TrainRoute"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="containsRoute"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TravelPath"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="ShipOnly"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="FlightOnly"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="BusOnly"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="PublicTransportationMeans">
    <owl:disjointWith>
      <owl:Class rdf:ID="RentableVehicle"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TransportationMeans"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Paintball">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SportsActivity"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Mudfight"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="SummerSportsActivity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="WinterSportsActivity"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Sale">
    <owl:disjointWith>
      <owl:Class rdf:ID="Bungalow"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="CampingSite"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Hotel"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Accommodation"/>
  </owl:Class>
  <owl:Class rdf:ID="Playground">
    <owl:disjointWith>
      <owl:Class rdf:about="#SpaFacility"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Pool"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Jakuzi"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Facility"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SummerResort">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:ID="availableActivity"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#SummerSportsActivity"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Location"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Facility">
    <owl:disjointWith>
      <owl:Class rdf:about="#Location"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Route"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Accommodation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Accommodation"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="facilityHostedAt"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#TravelPackage"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#TravelPath"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Service"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TransportationMeans"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Extra facilities that may be available in a hotel or some other lodging facility</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Activity"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="CarRental">
    <owl:disjointWith>
      <owl:Class rdf:ID="MotorbikeRental"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#VehicleRental"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Location">
    <owl:disjointWith>
      <owl:Class rdf:about="#Route"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Activity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#TransportationMeans"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Accommodation"/>
    <owl:disjointWith rdf:resource="#Facility"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TravelPath"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A location is an area that is the destination of potential travelers. It may be as general as a town, an island, or a state</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Service"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#TravelPackage"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="FamilyAccommodation">
    <rdfs:subClassOf rdf:resource="#Accommodation"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="#availableFacility"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#Playground"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="AirTransportationMeans">
    <owl:disjointWith>
      <owl:Class rdf:ID="SeaTransportationMeans"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="LandTransportationMeans"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#PublicTransportationMeans"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Flight"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:ID="makesRoute"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="WinterPackageSale">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasDestination"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="WinterResort"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TravelPackage"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ShipRoute">
    <owl:disjointWith>
      <owl:Class rdf:ID="BusRoute"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Flight"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#TrainRoute"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Route"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Ship"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="routeIsMadeBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Jakuzi">
    <owl:disjointWith>
      <owl:Class rdf:about="#SpaFacility"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Pool"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Playground"/>
    <rdfs:subClassOf rdf:resource="#Facility"/>
  </owl:Class>
  <owl:Class rdf:about="#Ship">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SeaTransportationMeans"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="#makesRoute"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#ShipRoute"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#RentableVehicle">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TransportationMeans"/>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#PublicTransportationMeans"/>
  </owl:Class>
  <owl:Class rdf:ID="Jetskiing">
    <owl:disjointWith>
      <owl:Class rdf:ID="BeachVolley"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Surfing"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SummerSportsActivity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Activity">
    <owl:disjointWith>
      <owl:Class rdf:about="#Route"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#TravelPackage"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Location"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Location"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="activityOfferedAt"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#TravelPath"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Accommodation"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Activities that are available in a location (e.g, surfing in a summer resort). Activities may also be part of a travel package</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Service"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:disjointWith rdf:resource="#Facility"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TransportationMeans"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#SportsActivity">
    <rdfs:subClassOf rdf:resource="#Activity"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="RelaxingActivity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="AdventureActivity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="SightSeeing"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#TravelPath">
    <owl:disjointWith>
      <owl:Class rdf:about="#Service"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#containsRoute"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:disjointWith rdf:resource="#Accommodation"/>
    <owl:disjointWith rdf:resource="#Activity"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Route"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#TravelPackage"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Facility"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TransportationMeans"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Location"/>
  </owl:Class>
  <owl:Class rdf:about="#CampingSite">
    <owl:disjointWith>
      <owl:Class rdf:about="#Bungalow"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Hotel"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Sale"/>
    <rdfs:subClassOf rdf:resource="#Accommodation"/>
  </owl:Class>
  <owl:Class rdf:about="#Bungalow">
    <rdfs:subClassOf rdf:resource="#Accommodation"/>
    <owl:disjointWith rdf:resource="#CampingSite"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Hotel"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Sale"/>
  </owl:Class>
  <owl:Class rdf:about="#WinterResort">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="#availableActivity"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#WinterSportsActivity"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="#Location"/>
  </owl:Class>
  <owl:Class rdf:ID="RentableMotorbike">
    <owl:disjointWith>
      <owl:Class rdf:ID="RentableCar"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#RentableVehicle"/>
  </owl:Class>
  <owl:Class rdf:about="#SpaFacility">
    <owl:disjointWith>
      <owl:Class rdf:about="#Pool"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Playground"/>
    <owl:disjointWith rdf:resource="#Jakuzi"/>
    <rdfs:subClassOf rdf:resource="#Facility"/>
  </owl:Class>
  <owl:Class rdf:ID="CaveExploration">
    <owl:disjointWith>
      <owl:Class rdf:ID="Climbing"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Safari"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#AdventureActivity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#AdventureActivity">
    <owl:disjointWith>
      <owl:Class rdf:about="#RelaxingActivity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#SightSeeing"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#SportsActivity"/>
    <rdfs:subClassOf rdf:resource="#Activity"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Activities for people who are into danger, mystery and wild life</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Service">
    <owl:disjointWith rdf:resource="#Accommodation"/>
    <owl:disjointWith rdf:resource="#Activity"/>
    <owl:disjointWith rdf:resource="#Location"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Services that may be available on location for tourists, like car rental. A service may be included in a travel package</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#TravelPackage"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#TravelPath"/>
    <owl:disjointWith rdf:resource="#Facility"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TransportationMeans"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Route"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#SeaTransportationMeans">
    <owl:disjointWith rdf:resource="#AirTransportationMeans"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#LandTransportationMeans"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#PublicTransportationMeans"/>
  </owl:Class>
  <owl:Class rdf:about="#Route">
    <owl:disjointWith rdf:resource="#Accommodation"/>
    <owl:disjointWith rdf:resource="#Activity"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TransportationMeans"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Facility"/>
    <owl:disjointWith rdf:resource="#Service"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A scheduled course for a public transportartion means (eg a flight for airplanes). It is described by the endpoint locations of the course, the date and time when it is scheduled to take place, and the transportation means which makes the route.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#TravelPackage"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Location"/>
    <owl:disjointWith rdf:resource="#TravelPath"/>
  </owl:Class>
  <owl:Class rdf:about="#TravelPackage">
    <owl:disjointWith rdf:resource="#Facility"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="startLocation"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasGoingTravelPath"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Service"/>
    <owl:disjointWith rdf:resource="#TravelPath"/>
    <owl:disjointWith rdf:resource="#Location"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TransportationMeans"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Activity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#hasAccommodation"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasReturnPath"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Accommodation"/>
    <owl:disjointWith rdf:resource="#Route"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >An object of this class contains all required arrangements (i.e. lodging and transportation) for a specific destination only. It may optionally include additional services and activities offered in the destination</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#hasDestination"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#VehicleRental">
    <rdfs:subClassOf rdf:resource="#Service"/>
    <owl:disjointWith rdf:resource="#Tour"/>
  </owl:Class>
  <owl:Class rdf:ID="Bus">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#LandTransportationMeans"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
